{
  "name": "@status-im/icons",
  "version": "0.0.1",
  "types": "src/index.tsx",
  "main": "src/index.tsx",
  "private": true,
  "files": [
    "types",
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch --mode development",
