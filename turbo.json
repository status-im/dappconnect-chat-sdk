{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "web#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "website#build": {
      "dependsOn": ["^build"],
      "env": ["TAMAGUI_TARGET"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "components#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "storybook-static/**"]
    },
    "dev": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "typecheck": {
      "outputs": []
    },
    "test": {
      "outputs": [],
      "dependsOn": []
    },
    "clean": {
      "cache": false
    }
  },
  "globalEnv": ["GITHUB_TOKEN"],
  "globalDependencies": ["tsconfig.base.json"]
}
